{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strict": false,
    "importsNotUsedAsValues": "remove",
    "baseUrl": ".",
    "paths": {
      "@shared/*": ["./src/shared/*"],
      "@partials/*": ["./src/partials/*"],
      "@site-config": ["./site-config.ts"],
      "@layouts/*": ["./src/layouts/*"]
    }
  },
}
